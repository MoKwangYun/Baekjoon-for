package for¹®;

import java.util.Scanner;

public class num2 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a = scanner.nextInt();
		int b = scanner.nextInt();
		int c = scanner.nextInt();
		for(int i = 0; i < a; i++) {
			System.out.println(b+ c);
		}
	}
}
